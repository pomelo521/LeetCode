// 可能的題型: 嘗試用input去作出可能的排列組合
// https://leetcode.com/problems/generate-parentheses/
// https://leetcode.com/problems/permutations/

class Solution:
    def pseudo_code(self, nums: List[int]) -> List[List[int]]:
        result = []
        def backtracking(sub_nums, sub_result):
            // Stop condition
            if ...:
                result.append(sub_result)
                return
            
            for i in range(sub_len):
                if ...:
                    sub_result = ...
                    sub_nums = ...
                    backtracking(sub_nums, sub_result)
                else:
                    sub_result = ...
                    sub_nums = ...
                    backtracking(sub_nums, sub_result)
                
        backtracking(nums, [])
        return result
